add 0x00 0x04 AX

MOVERDERECHA:
obs r
je DX 0x01 MOVERABAJO
mov r
je 0x00 0x00 MOVERDERECHA

MOVERABAJO:
obs d
je DX 0x01 MOVERIZQ
mov d
je 0x00 0x00 MOVERABAJO

MOVERIZQ:
obs l
je DX 0x01 MOVERARRIBA
mov l
je 0x00 0x00 MOVERIZQ

MOVERARRIBA:
obs u
je DX 0x01 VERIFICA 
mov u
je 0x00 0x00 MOVERARRIBA

VERIFICA:
sub AX 0x01 AX
jne AX 0x00 MOVERDERECHA

